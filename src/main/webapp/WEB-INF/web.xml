<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="true" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>UserManagerServlet</servlet-name>
    <servlet-class>com.lubocluod.touchwebcms.controller.UserManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserManagerServlet</servlet-name>
    <url-pattern>/usermanager</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RoleManagerServlet</servlet-name>
    <servlet-class>com.lubocluod.touchwebcms.controller.RoleManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleManagerServlet</servlet-name>
    <url-pattern>/rolemanager</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ChannelManagerServlet</servlet-name>
    <servlet-class>com.lubocluod.touchwebcms.controller.ChannelManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChannelManagerServlet</servlet-name>
    <url-pattern>/channelmanager</url-pattern>
  </servlet-mapping>
  
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
</web-app>